{
  "name": "tmp-minimal-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "module": "commonjs",
  "scripts": {
    "build": "tsup-node --config tsup.config.ts",
    "SERVER": "echo 'APP ONE'",
    "dev:server": "nodemon -r tsconfig-paths/register src/app_server/main.ts",
    "prod:server": "node dist/one.js",
    "build:server": "BUILD_APP=one tsup-node --config tsup.config.ts",
    "": "",
    "gen-grpc-types": "npx proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=grpc/_types/ grpc/**/*.proto",
    "deploy": "prisma migrate dev && yarn seeder",
    "migrate_dev": "prisma migrate dev",
    "migrate_prod": "npx prisma generate && prisma migrate deploy",
    "CLI": "echo 'CLI'",
    "cli:seeder": "ts-node -r tsconfig-paths/register src/cli/seeder.ts",
    "cli:send_test_email": "ts-node -r tsconfig-paths/register src/cli/send_test_email.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "adm-zip": "^0.5.10",
    "base-x": "^4.0.0",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.7",
    "hasha": "^5.2.2",
    "i18next": "^22.4.15",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "minimal2b": "^0.1.8",
    "nodemailer": "^6.9.1",
    "sharp": "^0.32.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.12",
    "@types/nodemailer": "^6.4.7",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "yargs": "^17.7.1"
