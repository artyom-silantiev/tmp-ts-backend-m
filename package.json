{
  "name": "tmp-minimal-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "module": "commonjs",
  "scripts": {
    "build": "tsup-node --config tsup.config.ts",
    "SERVER": "echo 'APP ONE'",
    "dev:server": "nodemon -r tsconfig-paths/register src/app_one/main.ts",
    "prod:server": "node dist/one.js",
    "build:server": "BUILD_APP=one tsup-node --config tsup.config.ts",
    "": "",
    "deploy": "prisma migrate dev && yarn seeder",
    "seeder": "ts-node -r tsconfig-paths/register src/cli/seeder.ts",
    "migrate_dev": "prisma migrate dev",
    "migrate_prod": "npx prisma generate && prisma migrate deploy",
    "gen-grpc-types": "npx proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=src/grpc/ grpc/**/*.proto"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "adm-zip": "^0.5.10",
    "base-x": "^4.0.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "cron": "^2.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.0",
    "hasha": "^5.2.2",
    "ioredis": "^5.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.0"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.8.13",
    "@types/adm-zip": "^0.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.10",
    "@types/yargs": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "yargs": "^17.7.1"
  },
  "author": "Artyom Silantiev",
  "license": "ISC"
}
